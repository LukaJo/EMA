    <UserSecretsId>57e16815-9336-4e06-8eb9-0969d333e828</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.3" />
